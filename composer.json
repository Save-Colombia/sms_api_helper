{
    "name": "hollow3464/sms_api_helper",
    "description": "SMS Api Helper",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Hollow3464\\SmsApiHelper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "David Chamorro",
            "email": "daved3464@gmail.com"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "friendsofphp/php-cs-fixer": "^3.14",
        "pestphp/pest": "^1.22"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.5",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    }
}
